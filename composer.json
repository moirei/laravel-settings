{
    "name": "moirei/settings",
    "description": "A simple solution for application settings.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Augustus",
            "email": "augustusokoye@moirei.com"
        }
    ],
    "keywords":[
        "MOIREI",
        "Laravel",
        "settings",
        "model settings",
        "user settings"
    ],
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^7|^8|^9",
        "illuminate/contracts": "^7|^8|^9",
        "laravel/helpers": "^1.5",
        "moirei/fields": "^1.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "orchestra/testbench": "^7.5",
        "illuminate/contracts": "^7|^8|^9",
        "phpunit/phpunit": "^9.5",
        "thled/pest-plugin-shorthands": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "MOIREI\\Settings\\": "src",
            "MOIREI\\Settings\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MOIREI\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
